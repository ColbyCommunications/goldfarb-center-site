{
	"name": "colbycommunications/goldfarb-center-site",
	"description": "Configuration project for the goldfarbcenter.colby.edu site",
	"type": "project",
	"require": {
		"johnpbloch/wordpress-core-installer": "^2.0",
		"johnpbloch/wordpress-core": "6.4.1",
		"wpackagist-theme/twentyseventeen": "^2.1",
		"wpackagist-plugin/classic-editor": "^1.4",
		"wpackagist-plugin/elementor": "3.18.0",
		"wpackagist-plugin/header-footer": "^3.1.3",
		"colbycommunications/colby-goldfarb-theme": "dev-master",
		"wpackagist-plugin/custom-post-type-ui": "1.15.1",
		"gillesgoetsch/acf-smart-button": "dev-master",
		"wpackagist-plugin/duplicate-post": "^4.3",
		"wpackagist-plugin/wordpress-seo": "21.6",
		"wpackagist-plugin/cloudflare": "4.12.2",
		"wpackagist-plugin/public-post-preview": "2.10.0",
		"wpackagist-plugin/wp-saml-auth": "2.1.4",
		"wpackagist-plugin/siteimprove": "2.0.4",
		"wp-cli/wp-cli-bundle": "^2.4",
		"psy/psysh": "^0.10.4",
		"colbycommunications/advanced-custom-fields-pro": "6.2.4",
		"colbycommunications/elementor-pro": "3.18.0",
		"colbycommunications/colby-ticket": "^1.0.0",
		"wpackagist-plugin/wp-search-with-algolia": "2.7.0",
		"vlucas/phpdotenv": "^5.4",
		"wpackagist-theme/hello-elementor": "2.7.1",
		"wpackagist-plugin/wp-nested-pages": "3.2.6"
	},
	"authors": [
		{
			"name": "Morty Mule",
			"email": "webmaster@colby.edu"
		}
	],
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/gillesgoetsch/acf-smart-button"
		},
		{
			"type": "git",
			"url": "https://github.com/ColbyCommunications/colby-goldfarb-theme"
		},
		{
			"type": "composer",
			"url": "https://wpackagist.org/"
		},
		{
			"type": "composer",
			"url": "https://satis.colby.edu"
		}
	],
	"extra": {
		"wordpress-install-dir": "web/wp",
		"installer-paths": {
			"web/wp-content/mu-plugins/{$name}": [
				"type:wordpress-muplugin",
				"wpackagist-plugin/wordpress-seo"
			],
			"web/wp-content/plugins/{$name}": [
				"type:wordpress-plugin"
			],
			"web/wp-content/themes/{$name}": [
				"type:wordpress-theme"
			]
		}
	},
	"config": {
		"allow-plugins": true
	}
}
